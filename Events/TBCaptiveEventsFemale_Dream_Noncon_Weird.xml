        <NotificationName>TB_slave_notification</NotificationName>
        <MultipleRestrictedListOfFlags>
        <RestrictedListOfFlags>Bestiality</RestrictedListOfFlags>
            <RestrictedListOfFlags>CanOnlyBeTriggeredByOtherEvent</RestrictedListOfFlags>
            <RestrictedListOfFlags>Captive</RestrictedListOfFlags>
        </MultipleRestrictedListOfFlags>
        <Options>
            <Option>
                <Order>0</Order>
                <MultipleRestrictedListOfConsequences>
                    <RestrictedListOfConsequences>ChangeHealth</RestrictedListOfConsequences>
                    <RestrictedListOfConsequences>Continue</RestrictedListOfConsequences>
                </MultipleRestrictedListOfConsequences>
                <OptionText>Try to push yourself off the creature.</OptionText>
                <HealthTotal>R 5 15</HealthTotal>
                <TriggerEvents>
                    <TriggerEvent>
                        <EventName>TB_dream_resist</EventName>
                        <EventWeight>50</EventWeight>
                    </TriggerEvent>
                    <TriggerEvent>
                        <EventName>TB_dream_insect_filled</EventName>
                        <EventWeight>100</EventWeight>
                    </TriggerEvent>
                    <TriggerEvent>
                        <EventName>TB_dream_insect_cage</EventName>
                        <EventWeight>100</EventWeight>
                    </TriggerEvent>
                    <TriggerEvent>
                        <EventName>TB_dream_insect_table</EventName>
                        <EventWeight>100</EventWeight>
                    </TriggerEvent>
                </TriggerEvents>
            </Option>
            <Option>
                <Order>1</Order>
                <MultipleRestrictedListOfConsequences>
                    <RestrictedListOfConsequences>Continue</RestrictedListOfConsequences>
                </MultipleRestrictedListOfConsequences>
                <OptionText>Try to wake up.</OptionText>
                <TriggerEventName>TB_dream_wake</TriggerEventName>
            </Option>
        </Options>
        <ReqCustomCode>true</ReqCustomCode>
        <SexualContent>true</SexualContent>
        <ReqHeroMinAge>18</ReqHeroMinAge>
    </CEEvent>
